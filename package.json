{
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run frontend && npm run server",
    "clean": "rm -rf dist",
    "develop": "concurrently 'npm run frontend/watch' 'npm run server/watch'",
    "frontend": "webpack",
    "frontend/watch": "webpack --watch",
    "lint": "eslint javascripts app test",
    "mocha": "mocha --compilers babel-register --require babel-polyfill --reporter dot --recursive",
    "server": "babel app --out-dir dist --source-maps",
    "server/watch": "npm run server -- --watch",
    "start": "node bin/start",
    "start/watch": "nodemon bin/start --watch dist --ignore dist/server/public",
    "test": "npm run lint && npm run mocha",
    "test/watch": "npm run mocha -- --watch"
  },
  "dependencies": {
    "express": "4.14.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "serialize-javascript": "1.3.0",
    "swig": "1.4.2",
    "underdog-pup": "1.0.x"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-react": "6.22.0",
    "babel-register": "6.22.0",
    "concurrently": "3.1.0",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "enzyme": "2.7.1",
    "eslint": "3.12.2",
    "eslint-config-underdog": "1.3.2",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.26.0",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
    "node-sass": "4.3.0",
    "nodemon": "1.11.0",
    "postcss-loader": "1.2.2",
    "react-addons-test-utils": "15.4.2",
    "sass-loader": "4.1.1",
    "source-map-support": "0.4.11",
    "style-loader": "0.13.1",
    "supertest": "3.0.0",
    "webpack": "1.14.0"
  }
}
